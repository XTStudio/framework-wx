<view style='{{style}}' animation='{{animation}}'>
    <image style='position:absolute; width: 100%; height: 100%' src="{{imageSource}}" mode="{{scaleMode}}"></image>
    <block wx:for="{{subviews}}" wx:key="index" wx:for-item="item">
        <factory wx:key="{{item.viewID}}" view="{{item}}"></factory>
    </block>
</view>